{
  "routes": [
    {
      "src": "/Login/(.*)",
      "methods": ["GET", "POST"],
      "dest": "/Login/index.html"
    },
    {
      "src": "/PaginaPrincipal/(.*)",
      "methods": ["GET"],
      "dest": "/PaginaPrincipal/PaginaPrincipal.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
